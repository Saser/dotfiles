Connection=wireless
Interface=wlp3s0
Security=wpa-configsection
ESSID=eduroam
IP=dhcp
WPAConfigSection=(
    'ssid="eduroam"'
    'key_mgmt=WPA-EAP'
    'eap=PEAP'
    'anonymous_identity="anonymous@chalmers.se"'
    'identity="<cid>@chalmers.se"'
    'password=hash:<hashed-password>'
    'ca_cert="/etc/ssl/certs/eduroam.pem"'
    'subject_match="/CN=eduroam.chalmers.se"'
    'phase2="auth=MSCHAPV2"'
    'priority=2')

# To generate hash:
# tr -d '[:space:]' | iconv -t utf16le | openssl md4
# write pass, then ctrl+d
